{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      { "src": "/api/v1/userMasters", "dest": "server.js" },
      { "src": "/api/v1/volunteers", "dest": "server.js" },
      { "src": "/api/v1/candidates", "dest": "server.js" },
      { "src": "/api/v1/*", "dest": "server.js" }
    ],
    "functions": {
        "api/**/*.js": {
        "runtime": "vercel-node@2.0.0"
        }
    }
  }
  